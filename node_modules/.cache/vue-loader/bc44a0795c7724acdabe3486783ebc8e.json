l for = \"Description\" > Description: </label>  \n            <textarea id = \"Description\" v-model=\"description\"></textarea>\n        </p> \n\n        <p>\n            <input type = \"submit\" value = \"Post\">  \n        </p>  \n    </form>\n</template>\n\n<script>\nexport default {\n    name: \"addRestaurantPosts\",\n    data () {\n        return {\n            id: this.$route.params.id,\n            food: \"\",\n            price: \"\",\n            description: \"\",\n\n            r_post: {\n                food: \"\",\n                price: \"\",\n                description: \"\",\n            }\n        }\n    },\n    methods: {\n        post: function () {\n            this.r_post.description= this.description;\n            this.r_post.price = this.price; \n            this.r_post.food = this.food;\n            this.$http.post('https://foodgram-8dac2.firebaseio.com/restaurants/' + this.id + '/posts.json', this.r_post).then(data=> {\n                console.log(data);\n            });\n            this.price=null;\n            this.food=null;\n            this.description=null;\n        }\n    }\n    \n}\n</script>\n\n<style scoped>\n\n    .restaurantposts {\n        margin: auto;\n        width: 50%;\n        padding: 20px;\n        font-family: Courier;\n    }\n\n</style>"]}]}