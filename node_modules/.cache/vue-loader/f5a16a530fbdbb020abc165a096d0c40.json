n        }\n    },\n    created() {\n\n        this.$http.get('https://foodgram-8dac2.firebaseio.com/restaurants/' + this.id + '/posts.json').then(data=> {\n           return data.data;\n        }).then(data=> {\n            var postArray = [];\n            for (let key in data) {\n                data[key].id = key \n                postArray.push(data[key]);\n            }\n            this.post = postArray;\n        });\n    },\n    mehtods: {\n        save_url() {\n            var save_url ='https://foodgram-8dac2.firebaseio.com/restaurants/' + this.id;\n            EventBus.$emit('url-saved', save_url);\n        }\n    },\n    computed: {\n    },\n    mixins: [searchMixin]\n}\n\n</script>\n\n<style scoped>\n    .single-restaurant {\n        margin: auto;\n        width: 50%;\n        padding: 30px;\n        text-align: center;\n\n    }\n\n    li {\n        list-style: none;\n        position:relative;\n    }\n\n    a {\n        text-decoration: none;\n        text-transform: uppercase;\n        color:black;\n    }\n\n    a:hover {\n        color: turquoise\n    }\n\n    \n</style>"]}]}